{
  "name": "Remote ROS Environment",
  "dockerComposeFile": [
    "./docker-compose.catkin.yml",
//    "./docker-compose.moveit.yml",
  ],
  "service": "ros-desktop-moveit",
  "workspaceFolder": "/home/catkin_ws/src/",
  "extensions": [
    "ms-python.python"
  ],
  "settings": {
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "shutdownAction": "stopCompose" // or "none"
}